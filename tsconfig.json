{
  "compilerOptions": {
    "skipLibCheck": true,
    "allowJs": true,
    "noImplicitAny": false,
    "checkJs": true,
    "strict": true,
    "noEmit": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "noUnusedParameters": true
  },
  "include": [
    "**/*",
  ],
}
